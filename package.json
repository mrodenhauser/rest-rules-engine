{
  "name": "bre-proto",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "nyc mocha",
    "merge": "swagger-merger -i ./specs/swagger.spec.json -o ./specs/swagger.json",
    "push": "git push && git push --tags",
    "git-update": "git remote update --prune",
    "commit": "git cz",
    "release": "standard-version && git push && git push --tags"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "~4.16.0",
    "express-jwt": "^5.3.1",
    "json-rules-engine": "^2.3.2",
    "lodash": "^4.17.11",
    "log-driver": "^1.2.7",
    "monk": "^6.0.6",
    "morgan": "~1.9.0",
    "morgan-json": "^1.1.0",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@octopusdeploy/grunt-octo": "0.0.50",
    "@octopusdeploy/octopackjs": "^0.1.0",
    "load-grunt-tasks": "^4.0.0",
    "octopus-deploy": "^4.1.4",
    "grunt": "^1.0.3",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-gitinfo": "^0.1.8",
    "chai": "^4.2.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "mocha": "^6.0.2",
    "mocha-bamboo-reporter": "^1.1.1",
    "mock-req-res": "^1.0.4",
    "nyc": "^13.3.0",
    "sinon": "^7.3.1",
    "standard-version": "^5.0.2",
    "swagger-merger": "^1.4.3",
    "validate-commit-msg": "^2.14.0"
  }
}
