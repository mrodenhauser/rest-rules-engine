{
  "tags": [
    "rule_sets"
  ],
  "summary": "Gets a rule set by id",
  "parameters": [
    {
      "in": "path",
      "name": "id",
      "schema": {
        "type": "string",
        "pattern": "^[a-fA-F0-9]{24}$"
      },
      "required": true,
      "description": "Id of the rule set you want to retrieve"
    }
  ],
  "responses": {
    "400": {
      "description": "Bad Request",
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/error_list"
          }
        }
      }
    },
    "200": {
      "description": "Ok",
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/named_rule_set"
          }
        }
      }
    },
    "401": {
      "description": "Unauthorized"
    },
    "404": {
      "description": "Not Found"
    }
  }
}
